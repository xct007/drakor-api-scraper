{
  "name": "drakor-api-scraper",
  "version": "1.0.0",
  "description": "get drakor series/movies",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "types": "./dist/@types/index.d.ts",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "./dist/@types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:all": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:types && node scripts/write-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xct007/drakor-api-scraper.git"
  },
  "author": "FrierenDv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xct007/drakor-api-scraper/issues"
  },
  "homepage": "https://github.com/xct007/drakor-api-scraper#readme",
  "devDependencies": {
    "@types/node": "^18.11.17",
    "rimraf": "^4.1.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.1"
  }
}
